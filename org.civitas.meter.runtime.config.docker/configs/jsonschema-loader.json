{
  ":configurator:resource-version": 1,
  
	"DefaultObjectMapperConfigurator~json": 
  	{
	    "type": "json",
		"codecFactoryConfigurator.target" : "(type=json)",
		"disableFeature": [
			"JsonWriteFeature.ESCAPE_FORWARD_SLASHES"
		],
		"enableFeatures": [
			"SerializationFeature.INDENT_OUTPUT"
		]
	},
	"DefaultCodecModuleConfigurator~json":
	{
		 "type": "json",
		 "serializers.target": "(component.name=JsonSchemaCodecEMFSerializers)",
		 "deserializers.target": "(component.name=JsonSchemaCodecEMFDeserializers)"
	},
	"DefaultCodecFactoryConfigurator~json": {
		"type": "json"
	}
}