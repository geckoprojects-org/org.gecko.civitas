-library: enable-emf

-buildpath: \
	org.osgi.service.typedevent,\
	org.gecko.emf.repository.api,\
	org.eclipse.fennec.qvt.api,\
	org.osgi.service.metatype.annotations
	
-workingset: handler

# Include model resources
-includeresource.model: model=model

Require-Capability: \
    osgi.extender;\
        filter:="(osgi.extender=emf.model)";\
        models:List<String>="/model"
