-include: dataatlas.runtime_base.bndrun

-runbundles.local: org.civitas.data.atlas.runtime.config.local,\
	org.civitas.glt.sensor.validation;version=snapshot,\
	org.civitas.pipeline.creator;version=snapshot

-runproperties.local: \
	base.path=${.},\
	logback.configurationFile=file:${.}/runtime/etc/logback.xml,\
	gecko.log.dir=${.}/runtime/logs,\
	org.apache.felix.configadmin.plugin.interpolation.secretsdir=${.}/runtime/secrets,\
	data=${.}/data/
	
-runtrace: true
