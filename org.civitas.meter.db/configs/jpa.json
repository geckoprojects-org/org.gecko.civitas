	"fennec.jpa.EORMMappingService~glt": {
		"fennec.jpa.eorm.strict": true,
		"fennec.jpa.eorm.mappingName": "glt",
		"fennec.jpa.eorm.eClasses": [
			"Building",
			"Contact"
		],
		"fennec.jpa.eorm.model.target": "(emf.name=glt)"
	},
	"daanse.jdbc.datasource.postgresql.DataSource~assets": {
		"name": "assets",
		".password": "postgres",
		"user": "postgres",
		"dbname": "usecase212_assets",
		"ports": 5432,
		"host": "localhost"
	},
	"fennec.jpa.EMPersistenceUnit~assets": {
		"fennec.jpa.ext.eclipselink.target-database": "PostgreSQL",
		"fennec.jpa.ext.eclipselink.ddl-generation": "none",
		"fennec.jpa.dataSource.target": "(name=assets)",
		"fennec.jpa.mapping.target": "(fennec.jpa.eorm.mapping=glt)",
