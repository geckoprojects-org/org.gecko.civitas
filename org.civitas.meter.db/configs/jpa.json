	},
	"daanse.jdbc.datasource.postgresql.DataSource~remoteMeter": {
		"name": "remoteMeter",
		".password": "postgres",
		"user": "postgres",
		"dbname": "usecase206_remote_meters",
		"ports": 5432,
		"host": "localhost"
	},
	"fennec.jpa.EMPersistenceUnit~remoteMeter": {
		"fennec.jpa.ext.eclipselink.target-database": "PostgreSQL",
		"fennec.jpa.ext.eclipselink.ddl-generation": "none",
		"fennec.jpa.dataSource.target": "(name=remoteMeter)",
		"fennec.jpa.mapping.target": "(fennec.jpa.eorm.mapping=metersource)",
		"fennec.jpa.persistenceUnitName": "remoteMeter"
	},
	"fennec.jpa.JPARepository~remoteMeter": {
