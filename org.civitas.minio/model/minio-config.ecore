<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="minioconfig" nsURI="https://civitas.org/minio/config/1.0.0"
    nsPrefix="minioconfig">
  <eClassifiers xsi:type="ecore:EClass" name="MinIOClientConfig">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Configuration for MinIO client connection settings"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="endpoint" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString" defaultValueLiteral="http://localhost:9000">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="MinIO server endpoint URL"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="accessKey" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString" defaultValueLiteral="minioadmin">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="MinIO access key for authentication"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="secretKey" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString" defaultValueLiteral="minioadmin">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="MinIO secret key for authentication"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="configurationId" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Unique identifier for this MinIO client configuration"/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="MinIOCSVDownloadServiceConfig">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Configuration for MinIO CSV download and processing service"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="bucketname" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="The name of the MinIO bucket to monitor for CSV files"/>
      </eAnnotations>
      <eAnnotations source="http://www.eclipse.org/emf/2002/ExtendedMetaData">
        <details key="osgi.config.property" value="bucketname"/>
        <details key="osgi.annotation.attribute" value="bucketname"/>
        <details key="documentation" value="Maps directly to OSGi Configuration Admin property 'bucketname' (no underscore conversion needed)"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="cron" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"
        defaultValueLiteral="0 */5 * * * *">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Cron expression for scheduling file checks (format: seconds minutes hours day month dayOfWeek)"/>
      </eAnnotations>
      <eAnnotations source="http://www.eclipse.org/emf/2002/ExtendedMetaData">
        <details key="osgi.config.property" value="cron"/>
        <details key="osgi.annotation.attribute" value="cron"/>
        <details key="documentation" value="Maps directly to OSGi Configuration Admin property 'cron' (no underscore conversion needed)"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="eClassUri" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="The URI of the EClass to use for parsing CSV data"/>
      </eAnnotations>
      <eAnnotations source="http://www.eclipse.org/emf/2002/ExtendedMetaData">
        <details key="osgi.config.property" value="eClassUri"/>
        <details key="osgi.annotation.attribute" value="eClassUri"/>
        <details key="documentation" value="Maps directly to OSGi Configuration Admin property 'eClassUri' (no underscore conversion needed)"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="configurationId" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Unique identifier for this CSV download service configuration"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="enabled" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"
        defaultValueLiteral="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Whether this download service configuration is enabled"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="clientTarget" eType="#//MinIOClientConfig">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Reference to the MinIO client configuration to use for connection"/>
      </eAnnotations>
      <eAnnotations source="http://www.eclipse.org/emf/2002/ExtendedMetaData">
        <details key="osgi.config.property" value="client.target"/>
        <details key="osgi.annotation.attribute" value="client_target"/>
        <details key="documentation" value="Maps to OSGi Configuration Admin property 'client.target' from annotation attribute 'client_target'"/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
</ecore:EPackage>
