{
    ":configurator:resource-version": 1,
    "MQTTService~write": {
        "brokerUrl": "$[env:MQTT_WRITE_PROTOCOL;default=$[prop:MQTT_WRITE_PROTOCOL;default=tcp]]://$[env:MQTT_WRITE_HOST;default=$[prop:MQTT_WRITE_HOST;default=localhost]]:$[env:MQTT_WRITE_PORT;default=$[prop:MQTT_WRITE_PORT;default=1883]]",
        "username": "$[env:MQTT_WRITE_USER;default=$[prop:MQTT_WRITE_USER;default=$[env:MQTT_FULL_USER;default=$[prop:MQTT_FULL_USER;default=]]]]",
        ".password": "$[env:MQTT_WRITE_PWD;default=$[prop:MQTT_WRITE_PWD;default=$[env:MQTT_FULL_PWD;default=$[prop:MQTT_FULL_PWD;default=]]]]",
        "maxThreads": 5,
        "maxInflight": 1000,
        "id": "local"
    }
}
