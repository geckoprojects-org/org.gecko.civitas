{
  ":configurator:resource-version": 1,

	 "MQTTService~full":
    {
        "brokerUrl" : "$[env:MQTT_FULL_PROTOCOL;default=$[prop:MQTT_FULL_PROTOCOL;default=tcp]]://$[env:MQTT_FULL_HOST;default=$[prop:MQTT_FULL_HOST;default=datainmotion.de]]:$[env:MQTT_FULL_PORT;default=$[prop:MQTT_FULL_PORT;default=1883]]",
        "username" : "$[env:MQTT_FULL_USER;default=$[prop:MQTT_FULL_USER;default=]]",
        ".password" : "$[env:MQTT_FULL_PWD;default=$[prop:MQTT_FULL_PWD;default=]]",
        "id" : "full",
        "maxInflight" : 1000
    }
}